{
  "name": "react-universally",
  "version": "1.0.0",
  "description": "An ultra low dependency node v6 universal react boilerplate.",
  "main": "build/server/main.js",
  "engines": {
    "node": "~6.3.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "dev": "node ./devServer",
    "build": "npm run clean && webpack --env.mode production --config webpack.client.config.js && webpack --env.mode production --config webpack.server.config.js",
    "start": "node build/server/main.js",
    "buildServer": "webpack --env.mode production --config webpack.server.config.js",
    "buildClient": "npm run clean && webpack --env.mode production --config webpack.client.config.js",
    "buildServerDev": "webpack --env.mode development --config webpack.server.config.js",
    "buildClientDev": "npm run clean && webpack --env.mode development --config webpack.client.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "compression": "1.6.2",
    "cssnano": "^3.7.3",
    "express": "4.14.0",
    "helmet": "2.1.1",
    "hpp": "0.2.1",
    "phone": "^1.0.8",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "2.5.2",
    "react-router-redux": "^4.0.5",
    "react-toolbox": "^1.0.2",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.3.1",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015-webpack": "6.4.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "chokidar": "1.6.0",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.11.0",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "2.0.0-beta.2",
    "fake-style-loader": "1.0.1",
    "fontgen-loader": "^0.2.1",
    "json-loader": "0.5.4",
    "node-notifier": "4.6.0",
    "node-sass": "^3.8.0",
    "normalize.css": "4.2.0",
    "path": "0.12.7",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "3.0.0-beta.2",
    "rimraf": "2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "0.13.1",
    "toolbox-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.19",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-node-externals": "1.2.0"
  }
}
